{
  "name": "bolidon",
  "displayName": "bolidon",
  "expo": {
    "extra": {
      "eas": {
        "projectId": "9843dc59-6b4f-4013-9ad4-3dea3e63f4b7"
      }
    },
    "name": "bolidon",
    "slug": "bolidon",
    "scheme": "bolidon",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon-all.png",
    "splash": {
      "image": "./assets/images/splash-logo-all.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "jsEngine": "hermes",
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "versionCode": 4,
      "icon": "./assets/images/app-icon-android-legacy.png",
      "package": "com.bolidon.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-icon-android-adaptive-foreground.png",
        "backgroundImage": "./assets/images/app-icon-android-adaptive-background.png"
      },
      "splash": {
        "image": "./assets/images/splash-logo-android-universal.png",
        "resizeMode": "contain",
        "backgroundColor": "#FFFFFF"
      },
      "googleServicesFile": "./google-services.json"
    },
    "ios": {
      "buildNumber": "4",
      "icon": "./assets/images/app-icon-ios.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.bolidon.app",
      "splash": {
        "image": "./assets/images/splash-logo-ios-mobile.png",
        "tabletImage": "./assets/images/splash-logo-ios-tablet.png",
        "resizeMode": "contain",
        "backgroundColor": "#FFFFFF"
      },
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "web": {
      "favicon": "./assets/images/app-icon-web-favicon.png",
      "splash": {
        "image": "./assets/images/splash-logo-web.png",
        "resizeMode": "contain",
        "backgroundColor": "#FFFFFF"
      },
      "bundler": "metro"
    },
    "plugins": [
      "expo-localization",
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": false,
            "deploymentTarget": "13.4"
          },
          "android": {
            "buildToolsVersion": "35.0.0",
            "minSdkVersion": 24,
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "kotlinVersion": "1.9.23",
            "ndkVersion": "26.1.10909125",
            "newArchEnabled": false
          }
        }
      ],
      "expo-font",
      [
        "expo-notifications",
        {
          "sounds": []
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "La aplicaci√≥n necesita permiso para acceder a tus fotos"
        }
      ]
    ],
    "experiments": {
      "tsconfigPaths": true
    }
  },
  "ignite": {
    "version": "9.10.1"
  }
}